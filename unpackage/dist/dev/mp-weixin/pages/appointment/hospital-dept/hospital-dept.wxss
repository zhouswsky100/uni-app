.warp {
  font-size: 30rpx;
  width: 100%;
  height: 100%;
  background-color: #fff;
}
.g-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 20rpx;
}
.g-item image {
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
    display: block;
    width: 120rpx;
    height: 120rpx;
    border-radius: 5000rpx;
}
.g-item .right {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    padding-left: 24rpx;
    overflow: hidden;
}
.g-item .title {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
}
.g-item .title text {
      font-size: 30rpx;
      color: #333;
}
.g-item .ops-box {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    font-size: 28rpx;
    color: #999;
    padding-top: 10rpx;
}
.g-item .ops-box .number {
      font-size: 26rpx;
      color: #999;
      margin-right: 15rpx;
}
.self-style {
  font-size: 34rpx;
}
.search {
  background-color: #fff;
  padding: 10rpx 20rpx;
  border-top: 20rpx solid #f2f2f2;
}
.search-box {
  height: 56rpx;
  border: 1rpx solid #ccc;
  padding: 0 15rpx;
}
.search-box view {
    margin-left: 10px;
}
.search-box view input[type=text] {
      height: 50rpx;
      text-indent: 20rpx;
      line-height: 50rpx;
}
.dept-list-box {
  height: 1000rpx;
}
.dept-list-box .dept-list-left {
    -webkit-box-flex: 2;
    -webkit-flex: 2;
            flex: 2;
    background-color: #eee;
}
.dept-list-box .dept-list-right {
    -webkit-box-flex: 3;
    -webkit-flex: 3;
            flex: 3;
}
.dept-list-box .dept-list-right .dept-item {
      padding: 15rpx;
}
.dept-list-box .dept-list-right .dept-item text {
        color: #333;
}
.left-aside {
  height: 100%;
  background: #f2f2f2;
}
.f-item {
  padding: 15rpx;
}
.f-item.active {
    background: #fff;
}

