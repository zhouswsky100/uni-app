<view class="warp"><block wx:for="{{allList}}" wx:for-item="groundItem" wx:for-index="groundIndex" wx:key="groundIndex"><block wx:if="{{groundItem.isFill}}"><view class="{{['all-list',groundItem.marginTop?'margin-top':'']}}"><view class="sub-list-box"><view class="sub-list solid-bottom"><view class="sub-list-content text-lg"><text class="cuIcon-titles text-blue"></text><text class="text-grey">{{groundItem.title}}</text></view></view></view><block wx:for="{{groundItem.inputList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-form-group"><view class="title">{{item.title}}</view><block wx:if="{{item.region}}"><picker value="{{i}}" range="{{item.region}}" data-index="{{index}}" data-key="{{groundIndex}}" data-event-opts="{{[['change',[['PickerChange',['$event']]]]]}}" bindchange="__e"><view class="{{['picker',(item.value!='')?'selfss':'']}}">{{''+(item.value==''?item.placeholder:item.region[item.checkedIndex])+''}}</view></picker></block><block wx:else><block wx:if="{{item.date}}"><picker mode="date" value="{{item.value==''?item.date:item.value}}" start="1920-01-01" end="2020-12-31" data-index="{{index}}" data-key="{{groundIndex}}" data-event-opts="{{[['change',[['DateChange',['$event']]]]]}}" bindchange="__e"><view class="{{['picker',(item.value!='')?'selfss':'']}}">{{''+(item.value==''?item.date:item.value)+''}}</view></picker></block><block wx:else><input class="text-gray" placeholder="{{item.placeholder}}" data-index="{{index}}" data-key="{{groundIndex}}" data-event-opts="{{[['blur',[['blur',['$event']]]]]}}" value="{{item.value}}" bindblur="__e"/></block></block></view></block></view></block></block><view class="padding flex flex-direction bg-white"><button data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="cu-btn bg-base margin-tb-sm lg text-white" bindtap="__e">提 交</button></view></view>